extern {
    func printf(s: string ...): int
};

func add(a: int, b: int ...) : int {
    let a = arg string;
    let b = arg int;
    let c = arg int;
    let d = arg int;
    printf("a: %s, b: %d, c: %d, d: %d\n", a, b, c, d);
    return b + c + d;
}

func main(): int {
    let res = add(1, 2, "hello", 2, 3, 4);
    printf("res: %d\n", res);
    return 0;
}
