extern {
    func printf(s: string 0...) : int
};

struct Hello {
    a = int,
    b = int
}

func initStruct(h: *struct Hello): unit {
    {h}.a = 12;
    {h}.b = 332;
}


func main(): int {
    let h = init Hello { a = 03, b = 2 };
    let k = ref h;
    initStruct(k);

    printf("Hello, World! %d %d\n", {@k}.a, {@k}.b);
    return 0;
}
